<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Pawnmorpher</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="Human"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Jaw"]/hediffGraphics</xpath>
					<value>
					    <EtherRedWolfMuzzle>
						    <path>Parts/WolfRed/RedWolf_Jaw</path>
						    <severity>
							    <a0.25>Parts/WolfRed/RedWolf_Jaw</a0.25>
							    <a0.15>Parts/Partials/PartialMuzzle/PartialMuzzle</a0.15>
							    <a0.0>Parts/Partials/PartialMuzzle/TinyMuzzle</a0.0>
						    </severity>
					    </EtherRedWolfMuzzle>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="Human"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="left ear" and inFrontOfBody="true"]/hediffGraphics</xpath>
					<value>
					    <EtherRedWolfEar>
						    <path>Parts/WolfRed/RedWolf_Ear_Left</path>
						    <severity>
							    <a0.5>Parts/WolfRed/RedWolf_Ear_Left</a0.5>
							    <a0.2>Parts/Partials/PointyEar/PointyEar_Left</a0.2>
							    <a0.0>Parts/None/None</a0.0>
						    </severity>
					    </EtherRedWolfEar>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="Human"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="left ear" and inFrontOfBody="false"]/hediffGraphics</xpath>
					<value>
					    <EtherRedWolfEar>
						    <path>Parts/WolfRed/RedWolf_Ear_Left_Behind</path>
						    <severity>
							    <a0.5>Parts/WolfRed/RedWolf_Ear_Left_Behind</a0.5>
							    <a0.2>Parts/Partials/PointyEar/PointyEar_Left_Behind</a0.2>
							    <a0.0>Parts/None/None</a0.0>
						    </severity>
					    </EtherRedWolfEar>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="Human"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="right ear" and inFrontOfBody="true"]/hediffGraphics</xpath>
					<value>
					    <EtherRedWolfEar>
						    <path>Parts/WolfRed/RedWolf_Ear_Right</path>
						    <severity>
							    <a0.5>Parts/WolfRed/RedWolf_Ear_Right</a0.5>
							    <a0.2>Parts/Partials/PointyEar/PointyEar_Right</a0.2>
							    <a0.0>Parts/None/None</a0.0>
						    </severity>
					    </EtherRedWolfEar>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="Human"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="right ear" and inFrontOfBody="false"]/hediffGraphics</xpath>
					<value>
					    <EtherRedWolfEar>
						    <path>Parts/WolfRed/RedWolf_Ear_Right_Behind</path>
						    <severity>
							    <a0.5>Parts/WolfRed/RedWolf_Ear_Right_Behind</a0.5>
							    <a0.2>Parts/Partials/PointyEar/PointyEar_Right_Behind</a0.2>
						    </severity>
					    </EtherRedWolfEar>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="Human"]/alienRace/generalSettings/alienPartGenerator/bodyAddons/li[bodyPart="Tail" or bodyPart="tail"]/hediffGraphics[./EtherWolfTail]</xpath>
					<value>
					    <EtherRedWolfTail>
						    <path>Parts/WolfRed/RedWolf_Tail</path>
						    <severity>
							    <a0.25>Parts/WolfRed/RedWolf_Tail</a0.25>
							    <a0.15>Parts/WolfRed/RedWolf_Tail_Short</a0.15>
							    <a0.0>Parts/None/None</a0.0>
						    </severity>
					    </EtherRedWolfTail>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
